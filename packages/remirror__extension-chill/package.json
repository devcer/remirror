{
  "name": "@remirror/extension-chill",
  "version": "1.0.1",
  "description": "The time to be chill.",
  "keywords": [
    "remirror",
    "extension"
  ],
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/remirror__extension-chill/extension-chill",
  "repository": {
    "type": "git",
    "url": "https://github.com/remirror/remirror.git",
    "directory": "remirror__extension-chill/extension-chill"
  },
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/extension-chill.esm.js",
      "require": "./dist/extension-chill.cjs.js",
      "browser": "./dist/extension-chill.esm.js",
      "types": "./dist/extension-chill.cjs.d.ts",
      "default": "./dist/extension-chill.esm.js"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/declarations/src/*.d.ts"
  },
  "main": "dist/remirror-extension-chill.cjs.js",
  "module": "dist/remirror-extension-chill.esm.js",
  "browser": {
    "./dist/remirror-extension-chill.cjs.js": "./dist/remirror-extension-chill.browser.cjs.js",
    "./dist/remirror-extension-chill.esm.js": "./dist/remirror-extension-chill.browser.esm.js"
  },
  "types": "dist/remirror-extension-chill.cjs.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@remirror/core": "^1.3.2"
  },
  "devDependencies": {
    "@remirror/pm": "^1.0.9"
  },
  "peerDependencies": {
    "@remirror/pm": "^1.0.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "@remirror": {
    "sizeLimit": "5 KB"
  }
}
